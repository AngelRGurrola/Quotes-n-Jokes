<%- include("partials/header.ejs") %>


<div class="card">
    <% if (locals.dynamicImage) { %>
  <img src="<%= dynamicImage %>" class="card-img-top" alt="..." style=" width: 100%; height:400px; object-fit:cover;">
    <% } %>
  <div class="card-body">
    <% if (locals.dynamicText) { %>
    <p class="card-text" id="typed-text" data-text="<%= dynamicText %>"></p>
    <% } else { %>
    <p class="card-text" id="typed-text"></p>
    <% } %>

  </div>
</div>


